#include<stdio.h>

void main()
{
    int num = 0;

    scanf("%d", &num);

    if (num % 7 == 0 && num > 21)
    {
        printf("Number is Divisible by 7 and less than 21");
    }
    else if (num % 7 == 0 && num > 21)
    {
        printf("Number is Divisible by 7 and greater than 21");
    }
    else
    {
        printf("Number is not divisible by 7");

    }
   
}